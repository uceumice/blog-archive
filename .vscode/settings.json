{
  "files.exclude": {
    "**/.cache": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/dist.type": true,
    "**/.turbo": true
  },
  "git.ignoreLimitWarning": true,
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    "class:list"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "prettier.documentSelectors": [
    "**/*.astro",
  ],
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}